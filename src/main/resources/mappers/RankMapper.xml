<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.project.footballkorea.files.dao.rankDAO">
 
 	<select id="rankListDAO" resultType="com.project.footballkorea.files.model.Rank">
 	
 		SELECT * FROM `clubRank`
 		
 		WHERE `leagueId` = 1
 		
 		ORDER BY `point` DESC
 	
 	
 	
 	</select>
 
 
 
 
 
 </mapper>